{
  "name": "@eddalabs/frontend-vite-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "npm run copy-contract-keys && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "copy-contract-keys": "node -e \"const fs=require('fs'),p=require('path');['keys','zkir'].forEach(d=>{const src=p.join('..','equity-registry-contract','src','managed','counter',d),dst=p.join('public','midnight','counter',d);fs.mkdirSync(dst,{recursive:true});fs.readdirSync(src).forEach(f=>fs.copyFileSync(p.join(src,f),p.join(dst,f)))});\""
  },
  "dependencies": {
    "@eddalabs/equity-registry-contract": "*",
    "@midnight-ntwrk/compact-js": "2.4.0",
    "@midnight-ntwrk/compact-runtime": "0.14.0",
    "@midnight-ntwrk/dapp-connector-api": "4.0.0",
    "@midnight-ntwrk/ledger-v7": "7.0.0",
    "@midnight-ntwrk/midnight-js-contracts": "3.0.0",
    "@midnight-ntwrk/midnight-js-fetch-zk-config-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-http-client-proof-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-indexer-public-data-provider": "3.0.0",
    "@midnight-ntwrk/midnight-js-network-id": "3.0.0",
    "@midnight-ntwrk/midnight-js-types": "3.0.0",
    "@tailwindcss/vite": "^4.1.10",
    "cross-fetch": "^4.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rxjs": "^7.8.1",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "1.0.3",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1"
  }
}
